<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>HolaMundoSaludo</servlet-name>
        <servlet-class>ejercicio1.HolaMundoSaludo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>numeros</servlet-name>
        <servlet-class>ejercicio1.numeros</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>numtabla</servlet-name><!-- no se usa-->
        <servlet-class>ejercicio1.numtabla</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NumerosEnTabla</servlet-name>
        <servlet-class>ejercicio1.NumerosEnTabla</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Form1</servlet-name>
        <servlet-class>ejercicio1.Form1</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>HolaMundoSaludo</servlet-name>
        <url-pattern>*.saludo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>numeros</servlet-name>
        <url-pattern>/numeros</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>numtabla</servlet-name><!-- no se usa-->
        <url-pattern>/numtabla</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NumerosEnTabla</servlet-name>
        <url-pattern>/Numtabla</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Form1</servlet-name>
        <url-pattern>/form1.proc</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
